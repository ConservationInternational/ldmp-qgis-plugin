{
    "prod_trajectory": {
        "Productivity trajectory (trend)": {
            "title": "prod_traj_trend_title",
            "ramp": {
                    "type": "zero-centered 2 percent stretch",
                    "shader": "interpolated",
                    "min": {"color": "#993304"},
                    "zero": {"color": "#F6F6EA"},
                    "max": {"color": "#008C79"},
                    "no data": {"value": 9999, "color": "#000000", "label": "prod_traj_trend_nodata"
                }
            }
        },
        "Productivity trajectory (significance)": {
            "title": "prod_traj_signif_title",
            "ramp": {
                "type": "categorical",
                "shader": "exact",
                "items": [
                    {"value": -3, "color": "#993304", "label": "prod_traj_signif_dec_99"},
                    {"value": -2, "color": "#BB7757", "label": "prod_traj_signif_dec_95"},
                    {"value": -1, "color": "#DDBBAB", "label": "prod_traj_signif_dec_90"},
                    {"value": 0, "color": "#F6F6EA", "label": "prod_traj_signif_zero"},
                    {"value": 1, "color": "#AAD8D2", "label": "prod_traj_signif_inc_90"},
                    {"value": 2, "color": "#55B2A5", "label": "prod_traj_signif_inc_95"},
                    {"value": 3, "color": "#008C79", "label": "prod_traj_signif_inc_99"},
                    {"value": 9999, "color": "#000000", "label": "prod_traj_signif_nodata"}
                ]
            }
        }
    },
